<body>
    <section class="dashboard-container">
        <aside>
            <div class="top-side">
                <a class="item" href=""><img src="../images/ALPHABILLS STRAIGHT 1.png" alt=""></a>
                <button class="toggle">
                    <div class="first-line"></div>
                    <div class="second-line"></div>
                    <div class="third-line"></div> 
                </button>
            </div>
            <div class="dashboard tags active">
                <a class="activer" href="/dashboard"><img src="images/tabler-icon-layout-dashboard.png" alt=""></a>
                <a class=" activer" href="/dashboard">Dashboard</a>
            </div>
            <div class="middle-side">
                <div class="tags">
                    <a  href="/airtime"><img src="images/tabler-icon-access-point.png" alt=""></a>
                    <a href="/airtime">Buy Airtime</a>
                </div>
                <div class="tags">
                    <a href="/data"><img src="images/tabler-icon-id.png" alt=""></a>
                    <a href="/data">buy data</a>
                </div>
                <div class="tags">
                    <a href="/billpayment"><img src="images/tabler-icon-battery-automotive.png" alt=""></a>
                    <a class="item" href="/billpayment">pay bills</a>
                </div>
                <div class="tags">
                    <a href="/wallet"><img src="images/tabler-icon-wallet.png" alt=""></a>
                    <a class="item" href="/wallet">wallet</a>
                </div>
                <div class="tags">
                    <a href="/profile"><img src="images/tabler-icon-address-book.png" alt=""></a>
                    <a class="item" href="/profile">profile</a>
                </div>
                <div class="tags">
                    <a href="/setting"><img src="images/tabler-icon-settings.png" alt=""></a>
                    <a class="item" href="/setting">Settings</a>
                </div>
            </div>
            <div class="bottom-side">
                <div class="tags log-out">
                    <a href="/sign-out"><img src="images/tabler-icon-logout.png" alt=""></a>
                    <a class="item" href="/sign-out">Log Out</a>
                </div>
            </div>
       </aside>

       <!-- mobile dashboard  -->
       
       <div>
        <button class="toggle-two">
            <div class="first-line"></div>
            <div class="second-line"></div>
            <div class="third-line"></div> 
           </button>
       </div>

       <nav class="nav">
            <div class="top-side">
                <a href=""><img src="images/ALPHABILLS STRAIGHT 1.png" alt=""></a>
            </div>
            <div class="dashboard tags active">
                <a class="activer" href="/dashboard"><img src="images/tabler-icon-layout-dashboard.png" alt=""></a>
                <a class=" activer" href="/dashboard">Dashboard</a>
