<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>AlphaBills | Account Settings</title>
    <link rel="stylesheet" href="css/aside.css">
    <link rel="stylesheet" href="css/account.css">
</head>
<body>
    <section class="dashboard-container">
        <%- include('../partials/aside') %>
       <!-- mobile dashboard end -->
        <main>
            <%- include('../partials/mobileHeader') %>
           <div class="dashboard-main-top">
                <h1>SETTINGS</h1>
                <div class="input-bar">
                    <div>
                        <img src="images/tabler-icon-bell.png" alt="">
                    </div>
                    <div class="profile-picture">
                        <img src="images/Frame 12.png" alt="profile-picture">
                    </div>
                </div>
           </div>


           <div class="main-setting">
                <div class="first-div">
                    <h1>ACCOUNTS</h1>
                    <div class="inner-1">
                        <div class="acct-statement">
                            <img src="images/Group 69.png" alt="">
                            <h1>ACCOUNT STATEMENT</h1>
                        </div>
                        <div class="disactive">
                            <img src="images/Group 70.png" alt="">
                            <h1>DEACTIVATE ACCOUNT</h1>
                        </div>
                    </div>
                    <div class="social-media">
                        <h1>FOLLOW US</h1>
                        <div class="links">
                            <div>
                                <img src="images/Group 71.png" alt="">
                            </div>
                            <div>
                                <img src="images/Group 72.png" alt="">
                            </div>
                            <div>
                                <img src="images/Group 73.png" alt="">
                            </div>
                            <div>
                                <img src="images/Group 74.png" alt="">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="second">
                    <h1>ABOUT</h1>
                    <div class="inner-1">
                        <div class="acct-statement">
                            <img src="images/Group 75.png" alt="">
                            <h1>ABOUT US</h1>
                        </div>
                        <div class="acct-statement">
                            <img src="images/Group 76.png"  alt="">
                            <h1>SUPPORT</h1>
                        </div>
                        <div class="acct-statement">
                            <img src="images/Group 77.png"  alt="">
                            <h1>TERMS & CONDITIONS</h1>
                        </div>
                        <div class="acct-statement">
                            <img src="images/Group 79.png"  alt="">
                            <h1>PRIVACY & POLICY</h1>
                        </div>
                    </div>
                </div>
           </div>
        </main>
    </section>
    

    <script src="js/dashboard.js"></script>
</body>
</html>